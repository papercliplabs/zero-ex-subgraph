{
    "name": "0x-protocol-mainnet",
    "license": "UNLICENSED",
    "scripts": {
        "codegen": "graph codegen",
        "build:mainnet": "graph build --network=mainnet",
        "deploy-hosted:mainnet": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-mainnet --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:mainnet": "source .env; graph deploy --studio 0x-protocol-mainnet --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:mainnet": "yarn codegen; yarn build:mainnet; yarn deploy-hosted:mainnet; yarn deploy-studio:mainnet --version-label $1",
        "build:optimism": "graph build --network=optimism",
        "deploy-hosted:optimism": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-optimism --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:optimism": "source .env; graph deploy --studio 0x-protocol-optimism --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:optimism": "yarn codegen; yarn build:optimism; yarn deploy-hosted:optimism; yarn deploy-studio:optimism --version-label $1",
        "build:arbitrum": "graph build --network=arbitrum-one",
        "deploy-hosted:arbitrum": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-arbitrum --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:arbitrum": "source .env; graph deploy --studio 0x-protocol-arbitrum --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:arbitrum": "yarn codegen; yarn build:arbitrum; yarn deploy-hosted:arbitrum; yarn deploy-studio:arbitrum --version-label $1",
        "build:base": "graph build --network=base",
        "deploy-hosted:base": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-base --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:base": "source .env; graph deploy --studio 0x-protocol-base --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:base": "yarn codegen; yarn build:base; yarn deploy-hosted:base; yarn deploy-studio:base --version-label $1",
        "build:polygon": "graph build --network=matic",
        "deploy-hosted:polygon": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-polygon --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:polygon": "source .env; graph deploy --studio 0x-protocol-polygon --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:polygon": "yarn codegen; yarn build:polygon; yarn deploy-hosted:polygon; yarn deploy-studio:polygon --version-label $1",
        "build:bnb": "graph build --network=bsc",
        "deploy-hosted:bnb": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-bnb --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:bnb": "source .env; graph deploy --studio 0x-protocol-bnb --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:bnb": "yarn codegen; yarn build:bnb; yarn deploy-hosted:bnb; yarn deploy-studio:bnb --version-label $1",
        "build:avalanche": "graph build --network=avalanche",
        "deploy-hosted:avalanche": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-avalanche --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:avalanche": "source .env; graph deploy --studio 0x-protocol-avalanche --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:avalanche": "yarn codegen; yarn build:avalanche; yarn deploy-hosted:avalanche; yarn deploy-studio:avalanche --version-label $1",
        "build:fantom": "graph build --network=fantom",
        "deploy-hosted:fantom": "source .env; graph deploy --node https://api.thegraph.com/deploy/ papercliplabs/0x-protocol-fantom --access-token $HOSTED_ACCESS_TOKEN",
        "deploy-studio:fantom": "source .env; graph deploy --studio 0x-protocol-fantom --deploy-key $STUDIO_DEPLOY_KEY",
        "auto-deploy:fantom": "yarn codegen; yarn build:fantom; yarn deploy-hosted:fantom; yarn deploy-studio:fantom --version-label $1",
